{
	"private": true,
	"name": "ntnu-csie-camp-app",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"prepare": "husky",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --ignore-path .gitignore --check **/*.{ts,js,json,yaml,yml,svelte,html,css} && eslint .",
		"format": "prettier --ignore-path .gitignore --write **/*.{ts,js,json,yaml,yml,svelte,html,css}",
		"changeset": "changeset",
		"upload": "pnpm build && wrangler pages deploy --project-name camp-backend .svelte-kit/cloudflare",
		"tail": "wrangler pages deployment tail --project-name camp-backend"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.1",
		"@cloudflare/workers-types": "^4.20240129.0",
		"@iconify/svelte": "^3.1.6",
		"@miniflare/cache": "^2.14.2",
		"@miniflare/r2": "^2.14.2",
		"@miniflare/storage-file": "^2.14.2",
		"@playwright/test": "^1.41.2",
		"@sentry/svelte": "^7.99.0",
		"@sentry/vite-plugin": "^2.13.0",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-cloudflare": "^4.1.0",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/typography": "^0.5.10",
		"@tsndr/cloudflare-worker-jwt": "==2.4.0",
		"@types/debug": "^4.1.12",
		"@types/pangu": "^4.0.2",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"autoprefixer": "^10.4.17",
		"better-sqlite3": "^9.4.0",
		"daisyui": "^4.6.1",
		"debug": "^4.3.4",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"hermes-mail": "^2.0.0",
		"husky": "^9.0.10",
		"kysely": "^0.27.2",
		"kysely-d1": "^0.3.0",
		"lint-staged": "^15.2.1",
		"octokit": "^3.1.2",
		"pangu": "^4.0.7",
		"postcss": "^8.4.33",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"svelte-confetti": "^1.3.2",
		"svelte-i18n": "^4.0.0",
		"svelte-markdown": "^0.4.1",
		"sveltekit-api": "^0.4.4",
		"sveltekit-db": "^0.2.1",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vitest": "^1.2.2",
		"wrangler": "^3.51.0",
		"zod": "^3.22.4"
	},
	"lint-staged": {
		"*.{ts,js,json,yaml,yml,svelte,html,css}": [
			"prettier --write"
		]
	},
	"packageManager": "pnpm@8.15.7"
}
